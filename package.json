{
  "name": "imdbrate",
  "version": "1.0.0",
  "description": "Telegram Bot for IMDb Movie Ratings This project is a Telegram bot that allows users to search for movies and get their IMDb ratings. The bot uses the OMDB API to retrieve movie data and returns the title, year of release, and IMDb rating.",
  "main": "index.js",
  "scripts": {
    "db:init": "node ./db/init",
    "db:drop": "node -e 'require(\"./db/drop\").dropDatabase()'",
    "db:migrate": "node -r esm ./node_modules/.bin/sequelize-cli db:migrate",
    "db:migrate:undo": "node -r esm ./node_modules/.bin/sequelize-cli db:migrate:undo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.6.9",
    "node-telegram-bot-api": "^0.61.0",
    "pg": "^8.10.0",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.0",
    "sharp": "^0.32.0",
    "string-similarity": "^4.0.4"
  },
  "devDependencies": {
    "esm": "^3.2.25"
  }
}
